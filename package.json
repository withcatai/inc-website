{
  "name": "inc-website",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "browser": "src/index.ts",
  "scripts": {
    "start": "vite --port 8080 --strictPort",
    "start:network": "vite --port 8080 --host 0.0.0.0 --strictPort",
    "start:noreload": "cross-env DISABLE_LIVE_RELOAD=true vite --port 8080 --strictPort",
    "start:noreload:network": "cross-env DISABLE_LIVE_RELOAD=true vite --port 8080 --host 0.0.0.0 --strictPort",
    "build": "vite build",
    "build:preview": "vite preview  --port 3002 --strictPort",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "format": "npm run lint:eslint -- --fix",
    "clean": "rm -rf ./node_modules ./dist /.eslintcache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withcatai/inc-website.git"
  },
  "author": "Gilad S.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/withcatai/inc-website/issues"
  },
  "homepage": "https://github.com/withcatai/inc-website#readme",
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.5",
    "classnames": "^2.5.1",
    "lifecycle-utils": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/node": "^22.19.2",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.0.0",
    "env-var": "^7.5.0",
    "eslint": "^9.26.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^7.2.7"
  }
}
